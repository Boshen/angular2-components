{
  "atom": {
    "rewriteTsconfig": false
  },
  "awesomeTypescriptLoaderOptions": {
    "babelCore": "",
    "babelOptions": {},
    "cacheDirectory": ".cache",
    "compiler": "typescript",
    "disableFastEmit": false,
    "doTypeCheck": true,
    "emitRequireType": false,
    "forkChecker": true,
    "forkCheckerSilent": false,
    "ignoreDiagnostics": [],
    "instanceName": "default",
    "reEmitDependentFiles": false,
    "skipDeclarationFilesCheck": false,
    "tsconfig": "tsconfig.json",
    "useBabel": false,
    "useCache": false,
    "usePrecompiledFiles": false
  },
  "buildOnSave": false,
  "compileOnSave": false,
  "compilerOptions": {
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "newLine": "LF",
    "noEmitHelpers": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5"
  },
  "exclude": [
    "node_modules"
    ]
}
